<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.talentica.tfdemo.tfMotionDetector.MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="108dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:text="User Acc :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblUserAcc"
        android:layout_width="216dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="108dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:text="Filtered Acc :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/lblFilteredValue"
        android:layout_width="216dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        android:text="labelb"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="108dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:text="isMoving :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView4" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:srcCompat="@color/colorPrimary" />

    <TextView
        android:id="@+id/txtView4"
        android:layout_width="108dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="10dp"
        android:text="Sigmoid"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/lblSigmoid"
        android:layout_width="216dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/txtView4"
        app:layout_constraintTop_toTopOf="@+id/txtView4" />

</android.support.constraint.ConstraintLayout>
